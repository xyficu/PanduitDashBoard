<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PanduitDashBoard"
        xmlns:ed="http://schemas.microsoft.com/expression/2010/drawing" x:Name="PanduitDashboardMain" x:Class="MainWindow"
        mc:Ignorable="d"
        Title="Dashboard-Panduit" Height="878.949" Width="800">
    <Window.Resources>
        <Style x:Key="GridStyle1" TargetType="{x:Type Grid}"/>
    </Window.Resources>
    <Grid Height="803" VerticalAlignment="Top">
        <Grid.RowDefinitions>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <DataGrid x:Name="dataGridUrgent" Margin="10,25,10,0" Height="229" VerticalAlignment="Top" VerticalContentAlignment="Stretch" HorizontalContentAlignment="Stretch" Foreground="#FFC90000" IsReadOnly="True" AlternationCount="2" AlternatingRowBackground="#FFFF9A9A"/>
        <Label x:Name="labelUrgentOrders" Content="Urgent Orders" HorizontalAlignment="Left" VerticalAlignment="Top" Height="25" Width="92" d:LayoutOverrides="HorizontalAlignment"/>
        <Label x:Name="labelNormalOrders" Content="Normal Orders" HorizontalAlignment="Left" Margin="0,259,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.443,0.586" Height="25" Width="106"/>
        <DataGrid x:Name="dataGridOrders" Margin="10,289,10,0" VerticalAlignment="Top" Height="300" IsReadOnly="True" AlternationCount="2" AlternatingRowBackground="#FF69DDFF"/>
        <Label x:Name="labelOrderCount" Content="label" HorizontalAlignment="Left" Margin="89.462,623.827,0,0" VerticalAlignment="Top" d:LayoutOverrides="HorizontalAlignment" Foreground="#FF6DB040" FontSize="18.667" RenderTransformOrigin="1.19,0.311"/>
        <Button x:Name="buttonRefresh" Content="Refresh" HorizontalAlignment="Left" Margin="679,609,0,0" VerticalAlignment="Top" Width="92" Height="25.24"/>
        <Label x:Name="labelPriceRequest" Content="label" HorizontalAlignment="Left" Margin="189.523,767.76,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.094,0.674" Foreground="#FFFF2F2F" FontSize="18.667" Height="30.24" Width="58.763"/>
        <Label x:Name="labelBooked" Content="label" Margin="346.136,623.827,383.407,0" VerticalAlignment="Top" RenderTransformOrigin="1.014,0.622" Foreground="#FF2FBDFF" FontSize="18.667"/>
        <Ellipse x:Name="shapeTotal" Fill="#FF79B553" HorizontalAlignment="Left" Height="41" Margin="48.463,0,0,104.466" Stroke="Transparent" VerticalAlignment="Bottom" Width="41" >
            <Ellipse.Effect>
                <DropShadowEffect/>
            </Ellipse.Effect>
        </Ellipse>
        <Label x:Name="label" Content="Total orders" HorizontalAlignment="Left" Margin="10.269,0,0,152.24" VerticalAlignment="Bottom"/>
        <Label x:Name="label1" Content="Booked orders" HorizontalAlignment="Left" Margin="251.939,0,0,152.24" VerticalAlignment="Bottom" d:LayoutOverrides="HorizontalAlignment"/>
        <ed:BlockArrow x:Name="arrow1" Fill="#FFF6FF2F" HorizontalAlignment="Left" Margin="107,657.534,0,117.932" Orientation="Right" Stroke="#FFB2DE6E" Width="182">
            <ed:BlockArrow.Effect>
                <DropShadowEffect/>
            </ed:BlockArrow.Effect>
        </ed:BlockArrow>
        <Label x:Name="label2" Content="Price Request orders" HorizontalAlignment="Left" Margin="48.463,0,0,5" VerticalAlignment="Bottom"/>
        <Ellipse x:Name="shapePriceRequest" Fill="#FFFF2F2F" HorizontalAlignment="Left" Height="41" Margin="180.463,0,0,35.24" Stroke="Transparent" VerticalAlignment="Bottom" Width="41" >
            <Ellipse.Effect>
                <DropShadowEffect/>
            </Ellipse.Effect>
        </Ellipse>
        <Ellipse x:Name="shapeBooked" Fill="#FF2FBDFF" HorizontalAlignment="Left" Height="41" Margin="305.136,0,0,104.466" Stroke="Transparent" VerticalAlignment="Bottom" Width="41" >
            <Ellipse.Effect>
                <DropShadowEffect/>
            </Ellipse.Effect>
        </Ellipse>
        <ed:LineArrow Fill="GreenYellow" HorizontalAlignment="Left" Height="54" Margin="82,0,0,50.466" Stroke="#FF8D8D8D" VerticalAlignment="Bottom" Width="80.463" StrokeThickness="10" StartCorner="BottomRight" StartArrow="Arrow" EndArrow="NoArrow"/>
        <ed:LineArrow Fill="#FFF6FF2F" HorizontalAlignment="Left" Height="49" Margin="233.463,0,0,50.466" Stroke="#FF8D8D8D" VerticalAlignment="Bottom" Width="82.673" StartCorner="TopRight" EndArrow="NoArrow" StartArrow="Arrow" StrokeThickness="10"/>


    </Grid>
</Window>
